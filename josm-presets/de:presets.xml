					text="Railway name"
					de.text="Streckenname"
					default=""
					delete_if_empty="true" />
				<space />
				<combo key="usage"
					text="Usage"
					de.text="Nutzung"
					values="main,branch,highspeed,industrial,military,tourism,test"
					display_values="Hauptstrecke,Nebenstrecke,Hochgeschwindigkeitsstrecke,Industriebahn,Militärbahn,Tourismusbahn,Teststrecke"
					default=""
					delete_if_empty="true" />
