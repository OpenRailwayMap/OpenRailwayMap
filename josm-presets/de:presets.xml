					delete_if_empty="true" />
				<combo key="railway:signal:speed_limit" text="Type of signal"
					de.text="Signaltyp" values="yes,zs3,dr:lf1/2,lf2,lf3,db:lf7,db:zs10"
					display_values="unbekannt,Zs 3,Lf 1/2 (DR),Lf 2,Lf 3,Lf 7 (DB),Zs 10 (DB)"
					default="" />
				<combo key="railway:signal:speed_limit:form" text="Signal form"
					de.text="Anzeigeart" values="light,sign"
					display_values="Lichtsignal,Tafel" default=""
					delete_if_empty="true" />
				<combo key="railway:signal:speed_limit:speed" default=""
				  text="Speed (km/h, leave blank for light signals)"
					de.text="Geschwindigkeit (km/h, leer lassen für Lichtsignale)"
