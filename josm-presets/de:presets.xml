<presets xmlns="http://josm.openstreetmap.de/tagging-preset-1.0">
	<group name="OpenRailwayMap" icon="signals.png">
		<group name="Gleise" icon="rail.png">
			<!--  preserved narrow_gauge light_rail subway tram miniature proposed construction disused abandoned razed Bahnhofsgleis Überleitgleis anschlussgleis-->
			<item name="Streckengleis / Hauptgleis" icon="rail.png" type="way">
				<label text="Streckengleis / Hauptgleis" />
				<key key="railway" value="rail" />
				<text key="ref" text="Line ref" de.text="Streckennummer (VzG-Nummer, z.B. '2379')" default="" delete_if_empty="true" />
				<text key="name" text="Track ref" de.text="Gleisnummer (im Bahnhof)" default="" delete_if_empty="true" />
				<text key="name" text="Railway name" de.text="Streckenname" default="" delete_if_empty="true" />
				<combo key="usage" text="Usage" de.text="Nutzung" values="main,branch,highspeed,industrial,military,tourism,test" display_values="Hauptstrecke,Nebenstrecke,Hochgeschwindigkeitsstrecke,Industriebahn,Militärbahn,Museumsbahn,Teststrecke" default="" delete_if_empty="true" />
				<combo key="electrified" text="Electrification" de.text="Elektrifizierung" values="yes,no,contact_line,rail" display_values="Ja,Nein,Oberleitung,Stromschiene" default="" delete_if_empty="true" />
				<combo key="railway:traffic_mode" text="Traffic mode" de.text="Verkehrsart" values="mixed,passenger,freight" display_values="Gemischt,Personenverkehr,Güterverkehr" default="" delete_if_empty="true" />
				<check key="highspeed" text="Highspeed traffic" de.text="Hochgeschwindigkeitsstrecke (~ >200km/h)" default="off" delete_if_empty="true" />
				<check key="lit" text="Lit" de.text="Beleuchtung" default="off" delete_if_empty="true" />
				<check key="railway:pzb" text="PZB" de.text="PZB" default="off" delete_if_empty="true" />
				<check key="railway:lzb" text="LZB" de.text="LZB" default="off" delete_if_empty="true" />
				<combo key="railway:etcs" text="ETCS" de.text="ETCS" values="yes,no,0,1,2,3" display_values="Ja,Nein,Level 0,Level 1,Level 2,Level 3" default="" delete_if_empty="true" />
				<text key="voltage" text="Voltage (Standard: 15000)" de.text="Spannung (Standard: 15000)" default="" delete_if_empty="true" />
				<text key="frequency" text="Frequency (Standard: 16.7)" de.text="Frequenz (Standard: 16.7)" default="" delete_if_empty="true" />
				<text key="gauge" text="Gauge" de.text="Spurweite" default="1435" delete_if_empty="true" />
				<check key="railway:ballastless" text="Ballastless" de.text="Feste Fahrbahn" default="off" delete_if_empty="true" />
				<!-- rack maxspeed railway:radio operator embankment cutting railway:bidirectional start_date end_date incline-->
			</item>
		</group>
		<group name="Signale" icon="signals.png">
