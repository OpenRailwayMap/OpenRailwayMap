		<item name="Kilo-/Hektometerbezeichnung" icon="milestone.png" type="node">
			<label text="Kilo-/hektometer indications are nodes on the track which
					they belong to. On lines with more than one track, every track gets a
					node with the tags."
				de.text="Kilo-/Hektometerbezeichnung werden als Punkt auf dem
					dazugehörigen Gleis eingetragen. Bei mehrgleisigen Strecken bekommt
					jedes Gleis einen Punkt mit den Tags."
					/>
			<key key="railway" value="milestone" />
			<text key="railway:position" default=""
				text="Distance indication (km)"
				de.text="Entfernungsangabe (km)"
				/>
			<text key="railway:position:exact" default="" delete_if_empty="true"
				text="Exact distance indication (km)"
				de.text="Exakte Entfernungsangabe (km)"
				/>
			<combo key="railway:milestone:emergency_brake_override"
				default="" delete_if_empty="true"
				text="Emergency brake override"
				de.text="Notfallbremsüberbrückung"
				values="yes,no"
				display_values="ja,nein"
				/>
			<combo key="railway:milestone:catenary_mast"
				default="" delete_if_empty="true"
				text="On catenary mast"
				de.text="Am Oberleitungsmast"
				values="yes,no"
				display_values="ja,nein"
				/>
		</item>
