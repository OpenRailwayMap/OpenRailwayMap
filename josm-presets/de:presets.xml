					text="Traffic mode"
					de.text="Verkehrsart"
					values="mixed,passenger,freight"
					display_values="Gemischt,Personenverkehr,Güterverkehr"
					default=""
					delete_if_empty="true" />
				<check key="highspeed"
					text="Highspeed traffic"
					de.text="Hochgeschwindigkeitsstrecke (~ >200km/h)"
					default="off"
					delete_if_empty="true" />
